{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "cache": {
          "disabled": true
        }
      }
    },
    "fresh-preview": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "cache": {
          "disabled": true
        }
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
